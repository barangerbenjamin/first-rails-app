<h1>Contact us</h1>

<form action="/contact">
  <input type="text" name="member" value="<%= params[:member] %>" placeholder="Search member">
  <input type="submit">
</form>

<ol>
  <% @members.each do |member| %>
    <li><%= member.capitalize %></li>
  <% end %>
</ol>

<%= link_to "Back to home", root_path %>